{
  "name": "cmake-project-template",
  "displayName": "cmake project template",
  "description": "the template of cmake project and third library",
  "version": "v1.7.0",
  "publisher": "1nchy",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:cpp"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cmake-project-template.cmake-project",
        "title": "New Project",
        "category": "CMake"
      },
      {
        "command": "cmake-project-template.cmake-library",
        "title": "New Lib",
        "category": "CMake"
      },
      {
        "command": "cmake-project-template.cmake-example",
        "title": "New Example",
        "category": "CMake"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "comment-json": "^4.2.4"
  }
}
